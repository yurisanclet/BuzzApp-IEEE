{
  "expo": {
    "name": "buzzapp-chat",
    "slug": "buzzapp-chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/purplebackground.png",
      "resizeMode": "cover",
      "backgroundColor": "#7B06B1"
    },
    "updates": {
      "fallbackToCacheTimeout": 2
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#7B06B1"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
